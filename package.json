{
  "name": "stock-market-cwa",
  "version": "1.0.0",
  "description": "Client web application for stock market",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack",
    "mock": "json-server --watch ./config/mock/mocks.json --routes ./config/mock/mockRoutes.json --middlewares ./config/mock/mockMiddleware.js",
    "dev:mock": "concurrently --kill-others \"npm run mock\" \"npm run dev\"",
    "lint": "eslint ./src/**/**.js",
    "lint:fix": "eslint ./src/**/**.js --fix",
    "test": "jest"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "styled-components",
    "cwa"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.5",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "es6-promise-promise": "^1.0.0",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "img-loader": "^3.0.2",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "mini-css-extract-plugin": "^1.3.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react-app-polyfill": "^2.0.0",
    "react-test-renderer": "^17.0.1",
    "react-testing-library": "^8.0.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^2.3.8",
    "url-loader": "^4.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/jest.config"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
